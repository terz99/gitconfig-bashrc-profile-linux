[alias]
	shelv = stash
	unsh = stash apply
	d = diff
	co = checkout
	cob = checkout -b
	b = branch
	st = status
	a = add
	aa = add -A .
	c = commit -m
	ac = !git add -A . && git commit -m
	amend = commit --amend -m
	p = push
	pu = pull
	lt = log --graph --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --numstat
	l = log --graph --pretty=format:'%Cred%h%Creset %Cgreen(%cr) %C(bold blue)<%an>%Creset%C(yellow)%d%Creset %n%s%n' --abbrev-commit
	rc = rebase --continue
	ra = rebase --abort
	r = rebase

